{
  "name": "skilld",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.28.2",
  "description": "Generate AI agent skills from npm package documentation",
  "author": {
    "name": "Harlan Wilton",
    "email": "harlan@harlanzw.com",
    "url": "https://harlanzw.com/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/harlan-zw/skilld"
  },
  "keywords": [
    "ai",
    "agent",
    "skills",
    "npm",
    "documentation",
    "claude",
    "llm",
    "cursor",
    "codex"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "skilld": "./dist/cli.mjs"
  },
  "files": [
    "dist",
    "skills"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "obuild",
    "dev:prepare": "obuild --stub",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "prepack": "pnpm run build",
    "prepare": "skilld --prepare || true"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "@huggingface/transformers": "^3.8.1",
    "@mdream/crawl": "^0.15.3",
    "citty": "^0.2.0",
    "code-chunk": "^0.1.13",
    "consola": "^3.4.2",
    "globby": "^16.1.0",
    "log-update": "^7.1.0",
    "mdream": "^0.15.3",
    "mlly": "^1.8.0",
    "oxc-parser": "^0.112.0",
    "p-limit": "^7.3.0",
    "retriv": "^0.8.3",
    "sqlite-vec": "^0.1.6",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.2.0",
    "@types/node": "^25.2.1",
    "@vitest/coverage-v8": "^4.0.18",
    "evalite": "^0.19.0",
    "obuild": "^0.4.26",
    "vitest": "^4.0.18"
  }
}
