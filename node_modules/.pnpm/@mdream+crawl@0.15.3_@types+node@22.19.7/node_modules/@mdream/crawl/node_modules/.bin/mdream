#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/harlan/pkg/skilld/node_modules/.pnpm/mdream@0.15.3/node_modules/mdream/bin/node_modules:/home/harlan/pkg/skilld/node_modules/.pnpm/mdream@0.15.3/node_modules/mdream/node_modules:/home/harlan/pkg/skilld/node_modules/.pnpm/mdream@0.15.3/node_modules:/home/harlan/pkg/skilld/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/harlan/pkg/skilld/node_modules/.pnpm/mdream@0.15.3/node_modules/mdream/bin/node_modules:/home/harlan/pkg/skilld/node_modules/.pnpm/mdream@0.15.3/node_modules/mdream/node_modules:/home/harlan/pkg/skilld/node_modules/.pnpm/mdream@0.15.3/node_modules:/home/harlan/pkg/skilld/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../mdream@0.15.3/node_modules/mdream/bin/mdream.mjs" "$@"
else
  exec node  "$basedir/../../../../../../mdream@0.15.3/node_modules/mdream/bin/mdream.mjs" "$@"
fi
