#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/harlan/pkg/skilld/node_modules/.pnpm/crawlee@3.15.3_@types+node@22.19.7/node_modules/crawlee/node_modules:/home/harlan/pkg/skilld/node_modules/.pnpm/crawlee@3.15.3_@types+node@22.19.7/node_modules:/home/harlan/pkg/skilld/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/harlan/pkg/skilld/node_modules/.pnpm/crawlee@3.15.3_@types+node@22.19.7/node_modules/crawlee/node_modules:/home/harlan/pkg/skilld/node_modules/.pnpm/crawlee@3.15.3_@types+node@22.19.7/node_modules:/home/harlan/pkg/skilld/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
